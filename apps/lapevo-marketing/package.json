{
  "name": "lapevo-marketing",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "wrangler pages dev dist",
    "astro": "astro",
    "db:migrate:local": "wrangler d1 migrations apply lapevo-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply lapevo-db --remote",
    "deploy": "npm run build && wrangler pages deploy dist --project-name=lapevo-marketing"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.0.0",
    "astro": "^5.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241230.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.0",
    "vite": "^6.4.1",
    "wrangler": "^4.57.0"
  }
}
